FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 440.15;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    roughCylinder
    {
        type            epsilonWallFunction;
        value           nonuniform 0();
    }
    sideWall
    {
        type            epsilonWallFunction;
        value           uniform 0.000765;
    }
    bottom
    {
        type            epsilonWallFunction;
        value           uniform 0.000765;
    }
    top
    {
        type            slip;
    }
    procBoundary5to2
    {
        type            processor;
        value           uniform 440.15;
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform 440.15;
    }
}


